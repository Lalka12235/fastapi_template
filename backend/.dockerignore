# Игнорируем директории виртуальных окружений
.venv/
venv/
env/

# Игнорируем кэши Python
__pycache__/
*.pyc
*.pyo
*.pyd

# Игнорируем файлы, специфичные для IDE и редакторов
.vscode/
.idea/
*.sublime-project
*.sublime-workspace

# Игнорируем файлы системы контроля версий
.git/
.gitignore

# Игнорируем файлы, создаваемые uv
.uv/    # Кэш uv

# Игнорируем файлы конфигурации окружения с секретами
# ОЧЕНЬ ВАЖНО: Никогда не включайте файлы .env с продакшн-секретами в образ!
#.env
#.env.*
#*.env

# Игнорируем логи и временные файлы
*.log
tmp/
temp/

# Игнорируем файлы, создаваемые операционной системой (macOS)
.DS_Store

# Игнорируем файлы данных, которые могут быть созданы локально
# и не должны быть частью образа
data/
uploads/
media/

# Игнорируем файлы тестов, если они не нужны в продакшн-образе
tests/
*.pytest_cache

# Игнорируем артефакты сборки, если они не нужны в финальном образе
dist/
build/
*.egg-info/

# Игнорируем файлы документации, если они не нужны в продакшн-образе
docs/
